<ehcache>
	
	<diskStore path="java.io.tmpdir"/>
	
	<cache 
		name="account"
		maxElementsInMemory="1000"
		eternal="false"
		timeToIdleSeconds="1800"
		timeToLiveSeconds="1800"
		overflowToDisk="true"></cache>
	<cache 
		name="post"
		maxElementsInMemory="1000"
		eternal="false"
		timeToIdleSeconds="30"
		timeToLiveSeconds="30"
		overflowToDisk="true"></cache>
	


</ehcache>


